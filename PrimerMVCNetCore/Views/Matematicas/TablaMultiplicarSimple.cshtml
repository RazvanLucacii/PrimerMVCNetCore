@model List<int>

<h1>
    Tabla Multiplicar
</h1>

<form method="post">
    <label>Introduzca cualquier numero: </label>
    <input type="number" name="numero" class="form-control" />
    <button class="btn btn-dark">
        Tabla Multiplicar
    </button>
</form>

@if (Model != null)
{
    <ul>
        @foreach (int num in Model)
        {
            <li>@num</li>
        }
    </ul>
}
